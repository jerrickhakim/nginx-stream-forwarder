# NGINX Stream Forwarder - Environment Variables
# Copy this file to .env and fill in your actual stream keys

# YouTube Live stream key (required for YouTube streaming)
YOUTUBE_STREAM_KEY=

# Twitch stream key (optional, for multi-platform streaming)
TWITCH_STREAM_KEY=

# X (Twitter) stream key (optional)
X_STREAM_KEY=

# Facebook stream key (optional)
FACEBOOK_STREAM_KEY=

# Kick stream key (optional)
KICK_STREAM_KEY=

# Application stream key (optional alternative to STREAM_KEY)
APP_STREAM_KEY=

# Stream authentication key (required - this is your secret key for RTMP authentication)
STREAM_KEY=

# Example:
# YOUTUBE_STREAM_KEY=1234-5678-90ab-cdef
# STREAM_KEY=my-secret-streaming-key 